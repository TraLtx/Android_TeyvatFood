<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teyvatfood" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeyvatFood" >
        <activity
            android:name=".activity.ChangeAccountInfo"
            android:exported="false" />
        <activity
            android:name=".activity.ViewCategory"
            android:exported="false" />
        <activity
            android:name=".activity.ManageAccount"
            android:exported="false" />
        <activity
            android:name=".activity.Search"
            android:exported="false" />
        <activity
            android:name=".activity.Checkout"
            android:exported="false" />
        <activity
            android:name=".activity.FoodDetail"
            android:exported="false" />
        <activity
            android:name=".activity.SignUp"
            android:exported="false" />
        <activity
            android:name=".activity.Login"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Activity_Splash"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>